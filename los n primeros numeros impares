// LONPRIMEROSNUEROSIMPARES.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;
void main ()
{
	int N,suma,i,cont;
	cout<<"ingresar N:";
	cin>>N;
	suma=0;
	cont=1;
	i=1;
	while(i<=N)
	{	suma=suma+cont;
	    cont=cont+2;
		i=i+1;
	}
	cout<<suma;
	getch();
}
