// seriesnaturales.cpp : Defines the entry point for the console application.
// 
#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"
 
using namespace std;

int main ()
{
	int suma,i,N;
	cout << "ingresar el valor de N:";
	cin>> N;
	suma=0;
	for (i=1;i<=N;i++)
		suma=suma+i*i;
	cout << "la suma de los "<< N <<"primeros numeros natuales es:"<< suma;
	getch();
	return 0;
}

	
